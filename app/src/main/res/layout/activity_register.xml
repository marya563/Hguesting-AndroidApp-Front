<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="0dp">
<include layout="@layout/toolbar"/>

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="32dp"
        android:layout_height="35dp"
        android:layout_marginBottom="388dp"
        android:layout_marginStart="24dp"
        android:layout_weight="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Firstname"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/img_13" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="32dp"
        android:layout_height="35dp"
        android:layout_marginBottom="468dp"
        android:layout_weight="1"
        android:layout_marginStart="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/lastname"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/img_13" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="27dp"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="192dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/img_44" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="24dp"
        android:layout_height="27dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="264dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/img_44" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="203dp"
        android:layout_height="115dp"
        android:layout_marginEnd="172dp"
        android:layout_marginBottom="600dp"
        android:fontFamily="@font/urbanistbold"
        android:text="Create your Account"
        android:textColor="@color/black"
        android:textSize="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


    <com.google.android.material.textfield.TextInputLayout

        android:id="@+id/password2"
        style="@style/ThemeOverlay.Material3.TextInputEditText.FilledBox"
        android:layout_width="333dp"
        android:layout_height="63dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="168dp"
        android:background="@drawable/customtextfield"
        android:hint="Confirm password"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        app:counterEnabled="true"
        app:counterMaxLength="10"
        app:endIconMode="password_toggle"
        app:errorEnabled="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent">


        >

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/editpassword2"
            android:layout_width="309dp"
            android:layout_height="38dp"
            android:background="@drawable/customtextfield"
            android:maxLength="10"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"

            tools:layout_editor_absoluteX="86dp"
            tools:layout_editor_absoluteY="514dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/role"
        style="@style/ThemeOverlay.Material3.TextInputEditText.FilledBox"
        android:layout_width="333dp"
        android:layout_height="63dp"
        android:layout_marginTop="156dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="515dp"
        android:background="@drawable/customtextfield"
        android:hint="Role"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        app:counterEnabled="true"
        app:counterMaxLength="10"


        app:endIconMode="clear_text"
        app:errorEnabled="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.333">

        <com.google.android.material.textfield.TextInputEditText

            android:id="@+id/editrole"
            android:layout_width="312dp"
            android:layout_height="35dp"
            android:background="@drawable/customtextfield"
            android:maxLength="10"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"


            tools:layout_editor_absoluteX="86dp"
            tools:layout_editor_absoluteY="167dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout

        android:id="@+id/lastname"
        style="@style/ThemeOverlay.Material3.TextInputEditText.FilledBox"
        android:layout_width="331dp"
        android:layout_height="65dp"
        android:layout_marginBottom="451dp"
        android:background="@drawable/customtextfield"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        app:counterEnabled="true"
        app:counterMaxLength="10"
        app:endIconMode="clear_text"
        app:errorEnabled="true"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.925"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <com.google.android.material.textfield.TextInputEditText

            android:id="@+id/editlastname"
            android:layout_width="312dp"
            android:layout_height="35dp"
            android:background="@drawable/customtextfield"
            android:hint="Lastname"
            android:maxLength="10"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            tools:layout_editor_absoluteX="82dp"
            tools:layout_editor_absoluteY="239dp" />

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/Firstname"
        style="@style/ThemeOverlay.Material3.TextInputEditText.FilledBox"
        android:layout_width="329dp"
        android:layout_height="66dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="376dp"
        android:background="@drawable/customtextfield"
        android:hint="Firstname"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        app:counterEnabled="true"
        app:counterMaxLength="10"
        app:endIconMode="clear_text"
        app:errorEnabled="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/editfirstname"
            android:layout_width="312dp"
            android:layout_height="35dp"
            android:background="@drawable/customtextfield"
            android:maxLength="10"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            tools:layout_editor_absoluteX="84dp"
            tools:layout_editor_absoluteY="290dp" />

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/Email"
        style="@style/ThemeOverlay.Material3.TextInputEditText.FilledBox"
        android:layout_width="333dp"
        android:layout_height="63dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="300dp"
        android:background="@drawable/customtextfield"
        android:hint="Email"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        app:counterEnabled="true"
        app:counterMaxLength="30"
        app:endIconMode="clear_text"
        app:errorEnabled="true"
        app:helperTextTextColor="@color/red"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <com.google.android.material.textfield.TextInputEditText

            android:id="@+id/emailedit"
            android:layout_width="312dp"
            android:layout_height="35dp"
            android:background="@drawable/customtextfield"
            android:maxLength="30"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"

            tools:layout_editor_absoluteX="88dp"
            tools:layout_editor_absoluteY="362dp" />

        <include layout="@layout/toolbar" />

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout

        android:id="@+id/password"
        style="@style/ThemeOverlay.Material3.TextInputEditText.FilledBox"
        android:layout_width="333dp"
        android:layout_height="63dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="240dp"
        android:background="@drawable/customtextfield"
        android:hint="Password"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        app:counterEnabled="true"
        app:counterMaxLength="10"
        app:endIconMode="password_toggle"
        app:errorEnabled="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/textinputeditpassword"
            android:layout_width="312dp"
            android:layout_height="35dp"
            android:background="@drawable/customtextfield"
            android:inputType="textEmailAddress"
            android:maxLength="10"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            tools:layout_editor_absoluteX="86dp"
            tools:layout_editor_absoluteY="434dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <Button
        android:id="@+id/signup"
        android:layout_width="272dp"
        android:layout_height="40dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="104dp"
        android:background="@drawable/custom_button"
        android:fontFamily="@font/urbanistbold"
        android:text="Sign up"
        android:textAllCaps="false"
        android:textColor="@color/yellow"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="247dp"
        android:layout_height="28dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/img_7" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="47dp"
        android:layout_height="32dp"
        android:layout_marginEnd="244dp"
        android:layout_marginBottom="19dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/img_4" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="32dp"
        android:layout_height="35dp"
        android:layout_marginEnd="171dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/img_6" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="47dp"
        android:layout_height="32dp"
        android:layout_marginEnd="105dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/img_5" />


    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginBottom="528dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_baseline_supervised_user_circle_24" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="28dp"
        android:layout_height="23dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="324dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/img_111" />


</androidx.constraintlayout.widget.ConstraintLayout>